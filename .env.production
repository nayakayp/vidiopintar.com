# Production Environment Variables
# Copy this file to .env and update with your production values

# Database Configuration
DB_PASSWORD=your_production_db_password
DB_USER=your_production_db_user
DB_HOST=your_production_db_host
DB_PORT=5432
DB_NAME=postgres

# Google Gemini AI
GOOGLE_GENERATIVE_AI_BASE_URL=https://gemini.fastlanemvp.com/v1beta
GOOGLE_GENERATIVE_AI_API_KEY=your_production_gemini_api_key

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://your-domain.com
NODE_ENV=production

# Google OAuth
GOOGLE_CLIENT_ID=your_production_google_client_id
GOOGLE_CLIENT_SECRET=your_production_google_client_secret

# Better Auth Configuration
BETTER_AUTH_SECRET=your_production_auth_secret
BETTER_AUTH_URL=https://your-domain.com

# API Configuration
API_BASE_URL=https://your-domain.com/api
API_X_HEADER_API_KEY=your_production_api_key

# Admin Configuration
ADMIN_MASTER_EMAIL=your-admin@email.com

# Transcript Service
TRANSCRIPT_API_BASE_URL=https://transcriptapi.com
TRANSCRIPT_API_KEY=your_production_transcript_api_key

# Analytics
UMAMI_ANALYTIC_URL=https://your-analytics-domain.com/script.js
UMAMI_ANALYTIC_WEBSITE_ID=your_production_website_id